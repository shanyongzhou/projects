page {
  background-color: #fbf9fe;
  height: 100%;
}
.page__hd{
    padding: 50rpx 50rpx 100rpx 50rpx;
    text-align: center;
}
.page__title{
    display: inline-block;
    padding: 20rpx 40rpx;
    font-size: 32rpx;
    color: #AAAAAA;
    border-bottom: 1px solid #CCCCCC;
}
.page__desc{
    display: none;
    margin-top: 20rpx;
    font-size: 26rpx;
    color: #BBBBBB;
}
.tr{
  padding-bottom: 30px;
}
.content{
  font-size: 30rpx;
  text-indent:2em;
  color:#8F8F8F
}
.text-left {
  color: #8B7765;
  display: inline-block;
  width: 200rpx;
  font-weight: 800;
}